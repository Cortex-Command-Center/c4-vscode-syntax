{
    "name": "c4ini",
    "displayName": "C4 INI Syntax",
    "description": "Adds syntax support for C4's variant of ini",
    "version": "0.0.1",
    "repository": {
        "url": "https://github.com/Wazubaba/c4-vscode-syntax"
    },
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "c4ini",
            "aliases": ["C4 Ini", "c4ini"],
            "extensions": [".ini"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "c4ini",
            "scopeName": "source.ini",
            "path": "./syntaxes/c4ini.tmLanguage.json"
        }]
    }
}